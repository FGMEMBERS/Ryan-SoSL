<?xml version="1.0"?>

<PropertyList>

 <sim include="Nasal/views.xml">
 
  <author>BARANGER Emmanuel (3D)</author>
  <aircraft-version>0.1</aircraft-version>
  <status>Alpha</status>
   
  <fuel-fraction>0.8</fuel-fraction>
  
  <startup>
   <splash-texture>Aircraft/Ryan-SoSL/ryan-sosl-splash.rgb</splash-texture>
  </startup>
  
  <systems>
   <electrical>
    <path>Aircraft/j3cub/cub-electrical.xml</path>
   </electrical>
  </systems> 

  <sound>
   <path>Aircraft/Ryan-SoSL/Sounds/ryan-sosl-sound.xml</path>
