   <rpm type="double">700</rpm>
  </engine>
 </engines>

 <input>
  <keyboard include="Nasal/ryan-sosl-keyboard.xml"/>
 </input>
 
 <nasal>
   <ryansosl>
     <file>Aircraft/Ryan-SoSL/Nasal/doors.nas</file>
   </ryansosl>
