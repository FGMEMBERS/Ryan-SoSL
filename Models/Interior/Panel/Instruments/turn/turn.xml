<?xml version="1.0"?>

<PropertyList>

  <path>turn.ac</path>

  <animation>
    <object-name>Face</object-name>
    <object-name>Needle1</object-name>
    <object-name>Background</object-name>
    <object-name>Ball</object-name>
    <object-name>Fond</object-name>
    <object-name>visse1</object-name>
    <object-name>visse2</object-name>
    <object-name>visse3</object-name>
    <object-name>visse4</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle1</object-name>
    <object-name>Background</object-name>
    <object-name>Ball</object-name>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Ball</object-name>
    <property>/instrumentation/slip-skid-ball/indicated-slip-skid</property>
    <min> -1.1 </min>
    <max>  1.1 </max>
    <interpolation>
      <entry><ind> -1.1 </ind><dep> -6.2 </dep></entry>
      <entry><ind>  1.1 </ind><dep>  6.2 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.00 </x-m>
      <y-m> 0.00 </y-m>
      <z-m> 0.22 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Needle1</object-name>
    <property>/instrumentation/turn-indicator/indicated-turn-rate</property>
    <factor>8.0</factor>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
