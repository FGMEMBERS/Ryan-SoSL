<?xml version="1.0"?>

<PropertyList>

  <path>asi.ac</path>

  <animation>
    <object-name>Face</object-name>
    <object-name>PointerTip</object-name>
    <object-name>Fond</object-name>
    <object-name>visse1</object-name>
    <object-name>visse2</object-name>
    <object-name>visse3</object-name>
    <object-name>visse4</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>PointerTip</object-name>
    <emission>
      <red>1.0</red>
      <green>0.2</green>
      <blue>0.0</blue>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>PointerTip</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>10</ind><dep>25</dep></entry>
      <entry><ind>60</ind><dep>80</dep></entry>
      <entry><ind>70</ind><dep>110</dep></entry>
      <entry><ind>80</ind><dep>145</dep></entry>
      <entry><ind>90</ind><dep>185</dep></entry>
      <entry><ind>100</ind><dep>210</dep></entry>
      <entry><ind>110</ind><dep>245</dep></entry>
      <entry><ind>120</ind><dep>270</dep></entry>
      <entry><ind>130</ind><dep>300</dep></entry>
      <entry><ind>140</ind><dep>320</dep></entry>
    </interpolation>
    <axis> 
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>  
    </center>
  </animation>

</PropertyList>
